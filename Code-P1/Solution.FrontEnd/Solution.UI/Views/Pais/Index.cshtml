@model List<string>

@{
    ViewData["Title"] = "Index";
    string nombre = "Fabian";
    //List<string> Pais = new List<string>() {"Rumania","Rusia","Eslovaquia" };
}

@inject Microsoft.AspNetCore.Hosting.IHostingEnvironment env

<h1>Index</h1>

<p>@nombre</p>

<environment include="Development">
    <p>"Estoy Desarrollo"</p>

</environment>

<environment include="Production">
    <p>"Estoy en Produccion"</p>

</environment>


<ul>
    @foreach (var pais in Model)
    {
        <li>@pais</li>
    }
</ul>

